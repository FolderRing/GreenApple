local function ANIYXC_fake_script() -- FakePeek.Do 
	local script = Instance.new('LocalScript', FakePeek)

	local fakepeek = false
	
	local RageBot = script.Parent
	
	
	script.Parent.MouseButton1Up:Connect(function()
		if RageBot.BackgroundColor3 == Color3.fromRGB(150,150,150) then
			RageBot.BackgroundColor3 = Color3.fromRGB(255,0,100)
			fakepeek = true
		else
			RageBot.BackgroundColor3 = Color3.fromRGB(150,150,150)
			fakepeek = false
		end
	end)
	
	while wait() do
		if fakepeek == true then
			local success,errorMessage = pcall(function()
			wait(math.random(99,501)/100)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(10,0,10)
			wait()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame - Vector3.new(10,0,10)
			end)
			end
	end
end
coroutine.wrap(ANIYXC_fake_script)()
local function NDFPXB_fake_script() -- AntiAim.Do 
	local script = Instance.new('LocalScript', AntiAim)

	local antiaim = false
	
	local RageBot = script.Parent
	
	
	script.Parent.MouseButton1Up:Connect(function()
		if RageBot.BackgroundColor3 == Color3.fromRGB(150,150,150) then
			RageBot.BackgroundColor3 = Color3.fromRGB(255,0,100)
			antiaim = true
		else
			RageBot.BackgroundColor3 = Color3.fromRGB(150,150,150)
			antiaim = false
		end
	end)
	
	game:GetService("UserInputService").InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.E then
			if antiaim == false then
				antiaim = true
				RageBot.BackgroundColor3 = Color3.fromRGB(255,0,100)
			else
				antiaim = false
				RageBot.BackgroundColor3 = Color3.fromRGB(150,150,150)
			end
		end
	end)
	
	while wait() do
		if antiaim == true then
			local success,errorMessage = pcall(function()
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(script.Parent.CurrentStrength.Value,0,script.Parent.CurrentStrength.Value)
				wait()
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame - Vector3.new(script.Parent.CurrentStrength.Value,0,script.Parent.CurrentStrength.Value)
				wait()
			end)
		end
	end
end
coroutine.wrap(NDFPXB_fake_script)()
